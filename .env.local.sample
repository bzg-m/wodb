# Copy this file to `.env.local` in the frontend root and fill the values.
# Vite recognizes files named `.env`, `.env.local`, etc. placed at the frontend root.
VITE_FIREBASE_API_KEY=your_firebase_api_key
VITE_FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your-project-id
# Optional: redirect URL after email-link sign-in (defaults to current origin)
VITE_FIREBASE_EMAIL_SIGNIN_REDIRECT=http://localhost:5173/
